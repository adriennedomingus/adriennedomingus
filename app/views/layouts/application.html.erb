<!DOCTYPE html>
<html>
<head>
  <%= favicon_link_tag %>
  <title>Adrienne Domingus</title>
  <link rel="stylesheet" type="text/css"  href="https://fonts.googleapis.com/css?family=Raleway">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav class="navbar navbar-default content">
    <div class="container-fluid">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <p><%= link_to "ADRIENNE DOMINGUS", root_path, class: "" %></p>
      </div>
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav navbar-right">
          <li class="navbar-link"><%= link_to "BLOG", blogs_path(:page => 1) %></li>
          <li class="navbar-link"><%= link_to "PORTFOLIO", projects_path %></li>
          <li class="navbar-link"><%= link_to "ABOUT & CONNECT", root_path %></li>
          <% if current_user %>
            <li class="navbar-link"><%= link_to "Log Out", logout_path, method: :delete %></li>
            <li class="navbar-link"><%= link_to "New Blog", new_user_blog_path %></li>
            <li class="navbar-link"><%= link_to "New Project", new_user_project_path %></li>
          <% end %>
        </ul>
      </div>
    </div>
  </nav>
<%= yield %>
</body>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-76149670-1', 'auto');
  ga('send', 'pageview');

</script>
</html>
