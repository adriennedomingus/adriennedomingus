<div class="container">
  <%= form_for @project, url: user_projects_path, method: :post do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
    <%= f.label :production %>
    <%= f.text_field :production, class: "form-control" %>
    <%= f.label :code %>
    <%= f.text_field :code, class: "form-control" %>
    <%= f.label :screenshot %>
    <%= f.text_field :screenshot, class: "form-control" %>
    <%= f.label :description %>
    <%= f.text_area :description, class: "form-control" %>
    <%= f.submit %>
  <% end %>
</div>
