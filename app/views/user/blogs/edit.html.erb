<div class="container">
  <%= form_for @blog, url: user_blog_path(@blog) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :date %>
    <%= f.text_field :date %>
    <%= f.label :content %>
    <%= f.text_area :content %>
    <%= f.submit %>
  <% end %>
</div>
